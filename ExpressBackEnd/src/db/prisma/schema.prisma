datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model spotify_user {
  id        String      @id @db.Uuid
  email     String   @unique
  first_name    String
  last_name     String
  password_hash     String  @db.VarChar(256)
}